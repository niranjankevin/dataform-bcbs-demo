config {
  type: "view",
  schema: "staging",
  tags: ["Staging"],
  description: "Cleaned version of stackoverflow.users table"
}

select
  id as user_id,
  cast(age as int) as age,
  creation_date,
  round(timestamp_diff(current_timestamp(), creation_date, day)/365) as user_tenure
from
  ${ref("users")}